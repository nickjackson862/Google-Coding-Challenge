  
import random

#factorial calculator function
def FactorialCalculator(num):
    if num<=0:
        print("number must be greater than zero")
    else:
        factorial = num
        while num >1:
            factorial = factorial * (num-1)
            num = num-1
        return(factorial)


#generate array
array = []
for i in range (random.randint(1,300)):
    array.append(random.randint(1,300))

#initiate solution arrays    
factorialArray = []
LRArray = []
N = len(array)
#Generate query numbers
Q = random.randint(1, N)


print(N)
for i in range(N):
    print(array[i], end = " ")
print()
print(Q)



#check for internal factorials and print
for i in array: 
    factorialVal = FactorialCalculator(i)
    for j in array:
        if factorialVal == j :
            LRArray.append(i)
            LRArray.append(j)
            factorialArray.append(factorialVal-i)
for i in range(len(LRArray)):
    print(LRArray[i], end = " ")
    
print()

factorialArray.sort()
if Q < len(factorialArray):
    for i in range(Q):
        print(factorialArray[i], end = " ")
else:
    for i in range(len(factorialArray)):
        print(factorialArray[i], end = " ")
